{
  "name": "teamneusta/formrecaptcha",
  "type": "typo3-cms-extension",
  "description": "Adds Google's reCAPTCHA to EXT:form",
  "license": ["GPL-2.0-or-later"],
  "homepage": "https://www.team-neusta.de/",
  "authors": [
    {
      "name": "Tobias Kretschmann",
      "email": "t.kretschmann@neusta.de",
      "homepage": "https://www.team-neusta.de",
      "role": "Developer"
    }
  ],
  "version": "2.0.1",
  "require": {
    "typo3/cms-core": "^10.4",
    "typo3/cms-form": "^10.4"
  },
  "suggest": {
    "vlucas/phpdotenv": "^5.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "formrecaptcha"
    }
  },
  "autoload": {
    "psr-4": {
      "Neusta\\Formrecaptcha\\": "Classes/",
      "Neusta\\Formrecaptcha\\Tests\\": "Tests/"
    }
  }
}
